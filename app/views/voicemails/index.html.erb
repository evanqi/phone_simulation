<p id="notice"><%= notice %></p>

<h1>Listing Voicemails</h1>

<table>
  <thead>
    <tr>
      <th>Audio</th>
      <th>Connection</th>
      <th>Created at</th>
      <th>Has been listened</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voicemails.each do |voicemail| %>
      <tr>
        <td><%= voicemail.audio %></td>
        <td><%= voicemail.connection %></td>
        <td><%= voicemail.created_at %></td>
        <td><%= voicemail.has_been_listened %></td>
        <td><%= link_to 'Show', voicemail %></td>
        <td><%= link_to 'Edit', edit_voicemail_path(voicemail) %></td>
        <td><%= link_to 'Destroy', voicemail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voicemail', new_voicemail_path %>
