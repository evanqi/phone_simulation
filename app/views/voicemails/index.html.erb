<p id="notice"><%= notice %></p>

<h1>Voicemails</h1>

<table class="table">
  <thead>
    <tr>
      <th>Audio</th>
      <th>Connection</th>
      <th>Created at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voicemails.each do |voicemail| %>
      <tr>
        <td><%= voicemail.audio %></td>
        <td><%= voicemail.connection_id %></td>
        <td>
          <%= voicemail.created_at %>
          &nbsp&nbsp
          <%= link_to 'Show', voicemail %>
          <%= link_to 'Destroy', voicemail, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Back', root_path %>

<br>
